void primeNumber1(int n);

int main()

{

int number;

scanf("%d",&number);

primeNumber1(number);

return 0;

}

void primeNumber1(int n)

{

//write your code here

for(int i=2;i<=n;i++)
{
     int flag=1;
    for( int j=2;j<i;j++)
    {
        if(i%j==0)
        {
            flag=0;
            break;
        }
    }
    if(flag==1)
    printf("%d ",i);

}
}
